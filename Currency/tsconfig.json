{
	"compilerOptions": {
		"strict": true,
		"allowSyntheticDefaultImports": true,
		"module": "es2015",
		"moduleResolution": "node",
		"target": "es6",
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"sourceMap": true,
		"experimentalDecorators": true,
		"strictPropertyInitialization": false
	}
}
